{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "cppbuild",
      "label": "C/C++: g++ Build disassembler",
      "command": "/usr/bin/g++",
      "args": [
        "-g",
        "${workspaceFolder}\\**.cpp",
        "-I",
        "${workspaceFolder}/headers",
        "-o",
        "${workspaceFolder}/myDiss"
      ],
      "options": {
        "cwd": "${fileDirname}"
      },
      "problemMatcher": ["$gcc"],
      "group": {
        "kind": "build",
        "isDefault": true
      },
      "detail": "compiler: /usr/bin/g++"
    }
  ]
}
